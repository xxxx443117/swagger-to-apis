{
  "name": "swagger-to-apis",
  "version": "2.0.7",
  "description": "Swagger transform APIs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "commonjs",
  "repository": "https://github.com/xxxx443117/swagger-to-apis",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "eslint": "eslint --fix src --ext .ts --max-warnings=0",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "build": "npm run eslint && npm run prettier && tsc --build && npm run cp",
    "cp": "ts-node cpTem.ts",
    "test": "jest",
    "prepare": "husky install && npm run build",
    "start": "ts-node src/index.ts",
    "test:start": "ts-node test/index.ts",
    "prepublishOnly": "npm run eslint && npm run version",
    "preversion": "npm run eslint",
    "version": "npm run prettier && git add -A src"
  },
  "keywords": [
    "swagger-to-apis",
    "swagger",
    "axios"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.1",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "node-fetch": "^2.6.7",
    "openapi-types": "^12.1.3",
    "prettier": "^3.0.1"
  }
}
